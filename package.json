{
  "name": "@data-structures-and-algorithms/heapq",
  "description": "Python's heap and priority queue library for JavaScript",
  "version": "3.1.0",
  "license": "AGPL-3.0",
  "author": "make-github-pseudonymous-again",
  "homepage": "https://data-structures-and-algorithms.github.io/heapq",
  "repository": {
    "url": "https://github.com/data-structures-and-algorithms/heapq",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/data-structures-and-algorithms/heapq/issues"
  },
  "keywords": [
    "heap",
    "heapq",
    "javascript",
    "js",
    "priority",
    "queue"
  ],
  "sideEffects": false,
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "build-docs": "esdoc",
    "build-gh-pages": "npm run build-docs",
    "commit-msg": "commitlint --edit",
    "cover": "c8 --all --src src --reporter=lcov npm test",
    "install-hooks": "husky install",
    "lint-config": "fixpack --dryRun",
    "lint-config-and-fix": "fixpack || fixpack",
    "postinstall": "npm run install-hooks",
    "postpublish": "pinst --enable",
    "prepare": "npm run build",
    "prepublishOnly": "pinst --disable",
    "release": "np",
    "test": "ava"
  },
  "dependencies": {
    "@failure-abstraction/error": "^6.0.1",
    "@iterable-iterator/iter": "^1.0.1",
    "@iterable-iterator/map": "^1.0.1",
    "@iterable-iterator/range": "^2.0.1",
    "@iterable-iterator/reduce": "^1.0.1",
    "@iterable-iterator/sorted": "^1.0.0",
    "@iterable-iterator/zip": "^1.0.1",
    "@total-order/primitive": "^1.0.1",
    "@total-order/reversed": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/register": "7.13.8",
    "@commitlint/cli": "13.1.0",
    "@iterable-iterator/list": "^1.0.1",
    "@js-library/commitlint-config": "0.0.4",
    "ava": "3.15.0",
    "babel-plugin-transform-remove-console": "6.9.4",
    "babel-plugin-unassert": "3.1.0",
    "babel-preset-power-assert": "3.0.0",
    "c8": "7.7.3",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "fixpack": "4.0.0",
    "husky": "7.0.1",
    "np": "7.4.0",
    "pinst": "2.1.6",
    "power-assert": "1.6.1",
    "regenerator-runtime": "0.13.9",
    "xo": "0.42.0"
  },
  "ava": {
    "files": [
      "test/src/**/*"
    ],
    "require": [
      "regenerator-runtime/runtime",
      "@babel/register"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
